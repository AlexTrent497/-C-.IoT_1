
#include <stdio.h>


int main(void)
{
    int num[5];
    for(int i = 0; i < 5; i++)
    {
        printf("Enter integer #%d: ", i+1);
        scanf("%d", &num[i]);
    }
    
    int min = num[0];
    for(int i = 1; i < 5; i++)
    {
            
        if(num[i] < min)
            min = num[i];            
    }
    
    printf("Smallest = %d\n", min);
    
 
    return 0;
}